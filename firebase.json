{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "style.sass"
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "https://oss.randomsense.jp/:path",
        "type": 301
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}